{
    "compilerOptions": {
        "outDir": "./built/",
        "sourceMap": true,
        "module": "es2015",
        "moduleResolution": "node",
        "allowSyntheticDefaultImports": true,
        "target": "es6",
        "allowJs": true,
        "noUnusedLocals": true,
        "noImplicitThis": false,
        "noImplicitAny": false,
        "strictNullChecks": false
    },
    "include": [
        "./public/**/*"
    ]
}
